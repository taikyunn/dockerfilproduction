{
  "family": "dockerfile-production-service",
  "containerDefinitions": [
      {
          "name": "dockerfile-production-service",
          "image": "",
          "cpu": 128,
          "memoryReservation": 128,
          "portMappings": [
              {
                  "containerPort": 3000,
                  "protocol": "tcp"
