{
  "family": "dockerfile-production-service",
  "containerDefinitions": [
      {
          "name": "dockerfile-production-service",
          "image": "926330672208.dkr.ecr.ap-northeast-1.amazonaws.com/dockerfile-production-go:latest",
          "cpu": 128,
          "memoryReservation": 128,
          "portMappings": [
              {
                  "containerPort": 3000,
                  "protocol": "tcp"
              }
          ],
          "command": [
              "/go/src/app/binary"
          ],
          "essential": true
      }
  ]
}
